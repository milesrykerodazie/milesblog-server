{
   "name": "blog-server",
   "version": "1.0.0",
   "description": "The server to handle blogging process",
   "main": "index.ts",
   "author": "Miles Ryker Odazie",
   "license": "MIT",
   "scripts": {
      "start": " node ./dist/index.js",
      "build": "concurrently \"tsc -p .\" \"npx tailwindcss build -i src/public/styles/tailwind.css -o src/public/styles/style.css\"",
      "dev": "concurrently \"npx tsc --watch\" \"nodemon -q src/index.ts\" \"npx tailwindcss build -i src/public/styles/tailwind.css -o src/public/styles/style.css\"",
      "build-tail": "npx tailwindcss build -i src/public/styles/tailwind.css -o src/public/styles/style.css --watch"
   },
   "dependencies": {
      "bcryptjs": "^2.4.3",
      "cloudinary": "^1.32.0",
      "cookie-parser": "^1.4.6",
      "cors": "^2.8.5",
      "date-fns": "^2.29.3",
      "dotenv": "^16.0.2",
      "express": "^4.18.1",
      "express-async-errors": "^3.1.1",
      "express-rate-limit": "^6.6.0",
      "helmet": "^6.0.0",
      "jsonwebtoken": "^8.5.1",
      "mongoose": "^6.6.1",
      "morgan": "^1.10.0",
      "nanoid": "^4.0.0",
      "nodemailer": "^6.7.8",
      "slugify": "^1.6.5",
      "ts-node": "^10.9.1",
      "uuid": "^9.0.0"
   },
   "devDependencies": {
      "@types/bcryptjs": "^2.4.2",
      "@types/cookie-parser": "^1.4.3",
      "@types/cors": "^2.8.12",
      "@types/express": "^4.17.14",
      "@types/express-rate-limit": "^6.0.0",
      "@types/helmet": "^4.0.0",
      "@types/jsonwebtoken": "^8.5.9",
      "@types/mongoose": "^5.11.97",
      "@types/morgan": "^1.9.3",
      "@types/node": "^18.7.18",
      "@types/nodemailer": "^6.4.6",
      "@types/uuid": "^8.3.4",
      "autoprefixer": "^10.4.12",
      "concurrently": "^7.4.0",
      "nodemon": "^2.0.20",
      "postcss": "^8.4.16",
      "tailwindcss": "^3.1.8",
      "typescript": "^4.8.3"
   }
}
